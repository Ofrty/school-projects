 000001F0  8B 5D 10				mov		ebx,			[ebp+16]
 000001F3  4B					dec		ebx
 000001F4  3B C3				cmp		eax,			ebx
 000001F6  7D 29				jge		endQuickMainPnt		; base case. we done. commence unzipping.

